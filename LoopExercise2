package selfProgramming;

public class LoopExercise2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int number;
		for (number = 2; number < 100; number++)
		{
			number = number*2;
			if (number<15)
				System.out.println("Nombor adalah lebih kecil daripada 15: " + number);
			else if (number > 15 && number < 100)
				System.out.println("Nombor adalah lebih besar daripada 15: " + number);
		}
	}

}

output:
Nombor adalah lebih kecil daripada 15: 4
Nombor adalah lebih kecil daripada 15: 10
Nombor adalah lebih besar daripada 15: 22
Nombor adalah lebih besar daripada 15: 46
Nombor adalah lebih besar daripada 15: 94
